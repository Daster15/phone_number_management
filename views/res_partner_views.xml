<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="view_partner_form_inherit_numbers" model="ir.ui.view">
    <field name="name">res.partner.form.inherit.number.pool</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="base.view_partner_form"/>
    <field name="arch" type="xml">
        <notebook position="inside">
            <page string="Phone Numbers">
                <field name="subscribed_number_ids">
                    <tree>
                        <field name="number"/>
                        <field name="number_type"/>
                        <field name="status"/>
                        <field name="source"/>
                    </tree>
                </field>
            </page>
        </notebook>
    </field>
</record>
</odoo>